<?xml version="1.0" encoding="utf-8"?>
<config>
	<name>1PixelOut MP3 Player</name>
	<creationDate>29 February 2012</creationDate>
	<author>Duvien</author>
	<authorUrl>http://duvien.com/blog/1pixelout-player-152-released</authorUrl>
	<copyright>(C) 2008 duvien.com</copyright>
	<license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<version>1.0.0</version>
	<description>Coming Soon...</description>
	<params>
		<param name="bg" type="text" default="f8f8f8" label="Background"
			description="Background color (see diagram)" />
		<param name="leftbg" type="text" default="eeeeee" label="Left Background"
			description="Left background (see diagram)" />
		<param name="lefticon" type="text" default="666666" label="Left Icon"
			description="Left icon (see diagram)" />
		<param name="rightbg" type="text" default="cccccc" label="Right Background"
			description="Right background (see diagram)" />
		<param name="rightbghover" type="text" default="999999"
			label="Right Background Hover" description="The color the right background will change to on mouseover" />
		<param name="righticon" type="text" default="666666" label="Right Icon"
			description="Right icon (see diagram)" />
		<param name="righticonhover" type="text" default="ffffff"
			label="Right Icon Hover" description="The color the right icon will change to on mouseover" />
		<param name="text" type="text" default="666666" label="Text"
			description="Text colour (see diagram)" />
		<param name="slider" type="text" default="666666" label="Slider"
			description="Slider colour (see diagram)" />
		<param name="track" type="text" default="FFFFFF" label="Track"
			description="Progress track colour (see diagram)" />
		<param name="border" type="text" default="666666" label="Border"
			description="Progress track border colour (see diagram)" />
		<param name="loader" type="text" default="9FFFB8" label="Loader"
			description="Loader bar colour (see diagram)" />
		<param name="loop" type="radio" default="no" label="Loop"
			description="The track will be looped indefinitely (default value is no)">
			<option value="no">no</option>
			<option value="yes">yes</option>
		</param>
	</params>
</config>