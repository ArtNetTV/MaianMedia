<?xml version="1.0" encoding="utf-8"?>
<config>
	<name>Flash MP3 Player</name>
	<creationDate>29 February 2012</creationDate>
	<author>Arelowo Alao</author>
	<authorUrl>http://www.longtailvideo.com/players/jw-flv-player/</authorUrl>
	<copyright>Are Times</copyright>
	<license>GNU/GPL</license>
	<version>1.0.0</version>
	<description>It's a MP3 player, intended for the diffusion on websites.
		It's open source, free and customizable.
	</description>
	<params>
		<param name="color" type="text" default="ffffff" label="Color" />
		<param name="bgcolor1" type="text" default="7c7c7c" label="Top Color" />
		<param name="bgcolor2" type="text" default="333333" label="Bottom Color" />
		<param name="width" type="text" default="200" label="Width" />
		<param name="height" type="text" default="20" label="Height" />
		<param name="style" type="textarea" default="a:active, a:focus {outline: 0};"
			label="Style" />
	</params>
</config>